trim(
  if(
    contains(triggerBody()?['subject'], '++'),
    split(triggerBody()?['subject'], '++')[0],
    if(
      contains(triggerBody()?['subject'], '--'),
      split(triggerBody()?['subject'], '--')[0],
      if(
        contains(triggerBody()?['subject'], '//'),
        split(triggerBody()?['subject'], '//')[0],
        triggerBody()?['subject']
      )
    )
  )
)
